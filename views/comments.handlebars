<div class="comments">
  {{#each this}}
    <div class="comment_card">
      {{! <h3 class="card-header"></h3> }}
      <div class="comment_body">
        <p class="text">
          {{comment_text}}
        </p>
        <p class="postedby">
          {{username}},
          {{created_at}}
        </p>
      </div>
    </div>
  {{/each}}
</div>
<script src="/js/comment.js"></script>